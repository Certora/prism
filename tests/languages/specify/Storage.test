methods {
    getThing(uint256) returns (uint256) envfree
    setThing(uint256, uint256) envfree
    addToSet(uint256, uint256) envfree
    setYoloSwagFoo(uint256) envfree
    setYoloSwagBar(uint256) envfree
    setYoloSwagMappedFoo(uint256, uint256) envfree
    setYoloSwagMappedBar(uint256, uint256) envfree
    setNeedfulNestedYoloSwagBar(uint256) envfree
    setMappedNeedfulNestedYoloSwagBar(uint256, uint256) envfree
    addToArray(uint256) envfree
    setArrayThenDelete(uint256,uint256) envfree
    setMappedYeteHot(uint256,uint128) envfree
    setMappedYeteDiggity(uint256,uint64) envfree
    setMappedYeteDawg(uint256,uint64) envfree
    // used below as hook pattern slot
    balanceOf(address) returns uint envfree
    setBalance(address,uint256) envfree
}

sort MyKey;

ghost sums(MyKey) returns uint;
ghost toMyKey(uint) returns MyKey;
ghost inSet(uint,uint) returns bool;
ghost fooSum() returns uint;
ghost barSum() returns uint;
ghost fooSums(MyKey) returns uint;
ghost barSums(MyKey) returns uint;
ghost needfulBarSum() returns uint;
ghost needfulBarSums(MyKey) returns uint;
ghost arraySum() returns uint;
ghost shadowArray(uint) returns uint;
ghost lastYeteHot() returns uint;
ghost lastYeteDiggity() returns uint;
ghost lastYeteDawg() returns uint;
ghost lastYete() returns uint;

definition updateSums(uint key, uint i) returns bool = forall MyKey otherKey. (toMyKey(key) != otherKey => sums@new(otherKey) == sums@old(otherKey)) && sums@new(toMyKey(key)) == sums@old(toMyKey(key)) + i;
definition updateFooSums(uint key, uint i) returns bool = forall MyKey otherKey. (toMyKey(key) != otherKey => fooSums@new(otherKey) == fooSums@old(otherKey)) && fooSums@new(toMyKey(key)) == fooSums@old(toMyKey(key)) + i;
definition updateBarSums(uint key, uint i) returns bool = forall MyKey otherKey. (toMyKey(key) != otherKey => barSums@new(otherKey) == barSums@old(otherKey)) && barSums@new(toMyKey(key)) == barSums@old(toMyKey(key)) + i;
definition updateNeedfulBarSums(uint key, uint i) returns bool = forall MyKey otherKey. (toMyKey(key) != otherKey => needfulBarSums@new(otherKey) == needfulBarSums@old(otherKey)) && needfulBarSums@new(toMyKey(key)) == needfulBarSums@old(toMyKey(key)) + i;
definition yete_hot(uint256 yete) returns uint256     = (0x00000000000000000000000000000000ffffffffffffffffffffffffffffffff & yete);
definition yete_diggity(uint256 yete) returns uint256 = (0x0000000000000000ffffffffffffffff00000000000000000000000000000000 & yete) >>> 128;
definition yete_dawg(uint256 yete) returns uint256    =  (0xffffffffffffffff000000000000000000000000000000000000000000000000 & yete) >>> 192;

// uses "balanceOf" which is also a function identifier
hook Sstore balanceOf[KEY address a] uint256 b STORAGE {
    assert true;
}

hook Sstore my_mapping[KEY uint key] uint i STORAGE {
    havoc sums assuming updateSums(key, i);
}

hook Sload uint i my_mapping[KEY uint key] STORAGE {
    havoc sums assuming updateSums(key, i);
}

hook Sstore my_map_of_sets[KEY uint set][KEY uint member] bool membership STORAGE {
    havoc inSet assuming forall uint otherSet. forall uint otherMember. ((otherSet != set || otherMember != member) => inSet@new(otherSet,otherMember) == inSet@old(otherSet, otherMember)) && inSet@new(set, member) == membership;
}

hook Sstore my_yolo_swag.(offset 0) uint i STORAGE {
    havoc fooSum assuming fooSum@new() == fooSum@old() + i;
}

hook Sstore my_yolo_swag.(offset 32) uint i STORAGE {
    havoc barSum assuming barSum@new() == barSum@old() + i;
}

hook Sstore my_yolo_swag_mapping[KEY uint key].(offset 0) uint i STORAGE {
    havoc fooSums assuming updateFooSums(key, i);
}

hook Sstore my_yolo_swag_mapping[KEY uint key].(offset 32) uint i STORAGE {
    havoc barSums assuming updateBarSums(key, i);
}

hook Sstore my_needful.(offset 32).(offset 32) uint i STORAGE {
    havoc needfulBarSum assuming needfulBarSum@new() == needfulBarSum@old() + i;
}

hook Sstore my_needful_mapping[KEY uint key].(offset 32).(offset 32) uint i STORAGE {
    havoc needfulBarSums assuming updateNeedfulBarSums(key, i);
}

hook Sstore my_uint_dynamic_array[INDEX uint index] uint n (uint old_n) STORAGE {
    havoc shadowArray assuming shadowArray@new(index) == n && (forall uint i. i != index => shadowArray@new(i) == shadowArray@old(i));
    require old_n < arraySum();
    havoc arraySum assuming arraySum@new() == arraySum@old() + (n - old_n);
}

hook Sstore my_yete_mapping[KEY uint key].(offset 0) uint yete STORAGE {
    havoc lastYeteDawg assuming lastYeteDawg@new() == yete_dawg(yete);
    havoc lastYete assuming lastYete@new() == yete;
    havoc lastYeteHot assuming lastYeteHot@new() == yete_hot(yete);
    havoc lastYeteDiggity assuming lastYeteDiggity@new() == yete_diggity(yete);
}

rule writeThingSums(uint key, uint n) {
    require n <= 25000;
    require forall MyKey k. sums(k) == 0;
    setThing@norevert(key, n);
    setThing@norevert(key, n);
    setThing@norevert(key, n);
    assert sums(toMyKey(key)) == 3 * n;
}

rule addToArraySum(uint n) {
    require n <= 25000;
    require arraySum() == 0;
    addToArray(n);
    addToArray(n);
    addToArray(n);
    assert arraySum() == 3 * n;
}

rule readThingSums(uint key) {
    uint m;
    uint n;
    uint o;
    require forall MyKey k. sums(k) == 0;
    m = getThing@norevert(key);
    n = getThing@norevert(key);
    o = getThing@norevert(key);
    require m <= 25000;
    require n <= 25000;
    require o <= 25000;
    assert sums(toMyKey(key)) == m + n + o;
}

rule setMembership {
    uint X;
    uint Y;
    uint a;
    uint b;
    require X != Y;
    require a != b;
    require forall uint set. forall uint member. !inSet(set,member);
    addToSet@norevert(X, a);
    assert inSet(X, a);
    assert !inSet(X,b);
    assert !inSet(Y,a);
    assert !inSet(Y,b);
}

rule writeYoloSwagFooSum(uint n) {
    require n <= 25000;
    require fooSum() == 0;
    setYoloSwagFoo@norevert(n);
    setYoloSwagFoo@norevert(n);
    setYoloSwagFoo@norevert(n);
    assert fooSum() == 3 * n;
}

rule writeYoloSwagBarSum(uint n) {
    require n <= 25000;
    require barSum() == 0;
    setYoloSwagBar@norevert(n);
    setYoloSwagBar@norevert(n);
    setYoloSwagBar@norevert(n);
    assert barSum() == 3 * n;
}

rule writeYoloSwagFooSums(uint key, uint n) {
    require n <= 25000;
    require forall MyKey k. fooSums(k) == 0;
    setYoloSwagMappedFoo@norevert(key, n);
    setYoloSwagMappedFoo@norevert(key, n);
    setYoloSwagMappedFoo@norevert(key, n);
    assert fooSums(toMyKey(key)) == 3 * n;
}

rule writeYoloSwagBarSums(uint key, uint n) {
    require n <= 25000;
    require forall MyKey k. barSums(k) == 0;
    setYoloSwagMappedBar@norevert(key, n);
    setYoloSwagMappedBar@norevert(key, n);
    setYoloSwagMappedBar@norevert(key, n);
    assert barSums(toMyKey(key)) == 3 * n;
}

rule writeNeedfulNestedYoloSwagBarSum(uint n) {
    require n <= 25000;
    require needfulBarSum() == 0;
    setNeedfulNestedYoloSwagBar@norevert(n);
    setNeedfulNestedYoloSwagBar@norevert(n);
    setNeedfulNestedYoloSwagBar@norevert(n);
    assert needfulBarSum() == 3 * n;
}

rule writeMappedNeedfulNestedYoloSwagBarSums(uint key, uint n) {
    require n <= 25000;
    require forall MyKey k. needfulBarSums(k) == 0;
    setMappedNeedfulNestedYoloSwagBar@norevert(key, n);
    setMappedNeedfulNestedYoloSwagBar@norevert(key, n);
    setMappedNeedfulNestedYoloSwagBar@norevert(key, n);
    assert needfulBarSums(toMyKey(key)) == 3 * n;
}

rule writeDynamicArrayThenDelete(uint n, uint length) {
    require n <= 25000;
    // 3 since BMC is set at 3
    require length <= 3;
    require length > 0;
    uint last = length - 1;
    setArrayThenDelete(n, length);
    assert shadowArray(0) == 0;
    assert shadowArray(last) == 0;
}

rule writeDynamicArrayThenDelete2(uint n, uint length) {
    require n <= 25000;
    require length <= 25000;
    require arraySum() == 0;
    setArrayThenDelete(n, length);
    assert arraySum() == 0;
}

rule writeMappedYeteHot(uint256 n, uint64 v) {
    setMappedYeteHot(n, v);
    assert v == lastYeteHot(); 
}

rule writeMappedYeteDiggity(uint256 n, uint64 v) {
    setMappedYeteDiggity(n, v);
    assert v == lastYeteDiggity();
}

rule writeMappedYeteDawg(uint256 n, uint64 v) {
    setMappedYeteDawg(n, v);
    assert v == lastYeteDawg();
}

rule setBalance(address a, uint256 b) {
    // trigger balanceOf hook in case some error gets triggered only
    // if hook gets used
    setBalance(a, b);
    assert true;
}

