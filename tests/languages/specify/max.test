definition MAX_UINT() returns uint = 2^256-1;
definition MAX_UINT256() returns uint = 2^256-1;
definition MAX_UINT160() returns uint = 2^160-1;
definition MAX_UINT128() returns uint = 2^128-1;
definition MAX_UINT96() returns uint = 2^96-1;
definition MAX_UINT64() returns uint = 2^64-1;
definition MAX_UINT32() returns uint = 2^32-1;
definition MAX_UINT16() returns uint = 2^16-1;
definition MAX_UINT8() returns uint = 2^8-1;


rule checkAll {
    assert max_uint == MAX_UINT();
    assert max_uint256 == MAX_UINT256();
    assert max_uint160 == MAX_UINT160();
    assert max_address == MAX_UINT160();
    assert max_uint128 == MAX_UINT128();
    assert max_uint96 == MAX_UINT96();
    assert max_uint64 == MAX_UINT64();
    assert max_uint32 == MAX_UINT32();
    assert max_uint16 == MAX_UINT16();
    assert max_uint8 == MAX_UINT8();
}

